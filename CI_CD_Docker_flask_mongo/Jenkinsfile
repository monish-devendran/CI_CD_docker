pipeline{
  agent any
  stages{
    stage("first-test")
    {
      steps{
        script {
            env.PATH = "$WORKSPACE"
        }
        echo "${env.PATH }"
      }
    }
    stage("deploy"){
        steps{
            sshagent(['docker-machine']) {
                // some block
                sh """
                ssh -o StrictHostKeyChecking=no ${env.PATH } dmonish3@34.72.181.130:/home/dmonish3/apps
                echo "now in remote vm"
                echo $path
                """
            }
        }
    }
  }
}
